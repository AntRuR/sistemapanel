/**
 * some resets
 */
table
	border-collapse: collapse
	border-spacing: 0

td,
th
	padding: 0


h1, 
h2, 
h3, 
h4, 
h5, 
h6
	font-size 100%
	font-weight normal

body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
code,
form,
fieldset,
legend,
input, 
button, 
textarea, 
th, 
td,
nav 
	margin 0
	padding 0
	position relative


  
li
	list-style none

